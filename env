#!/bin/sh
case ":${MYSELF_CONFIG}:" in
    *:"$HOME/.myself_config":*)
        ;;
    *)
        # Prepending path in case a system-installed rustc needs to be overridden
        export MYSELF_CONFIG="$HOME/.myself_config"
        export JAVA_HOME="/opt/jdk21"
        export GOPATH="/opt/go_path"
        export DOTNET_ROOT="/opt/dotnet8"
        export PATH="$PATH:/opt/go/bin:/opt/node/bin:/opt/jdk/bin:$MYSELF_CONFIG/script:$GOPATH/bin:$DOTNET_ROOT:$DOTNET_ROOT/tools"
        ;;
esac
